{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "version": "1.2.5",
  "content_scripts": [
    {
      "js": ["/content_scripts.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "/worker.js"
  },
  "permissions": ["contextMenus", "storage"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "icon.png",
    "16": "icon.png"
  },
  "default_locale": "en"
}
